{
  "net_name": "Geminet",
  "global":{
     "model_type": "PY",
      "weights_file": "../matchzoo/models/weights/geminet.wikiqa.weights",
      "save_weights_iters": 10,
      "num_iters": 200,
      "display_interval": 10,
      "test_weights_iters": 100,
      "optimizer": "adadelta",
      "learning_rate": 0.001
  },
  "inputs": {
    "share": {
        "text1_corpus": "../data/WikiQA/corpus_preprocessed.txt",
        "text2_corpus": "../data/WikiQA/corpus_preprocessed.txt",
        "text1_postag_corpus": "../data/WikiQA/corpus_postag_preprocessed.txt",
        "text2_postag_corpus": "../data/WikiQA/corpus_postag_preprocessed.txt",
        "use_dpool": true,
        "embed_size": 300,
        "embed_path": "../data/WikiQA/embed_glove_d300_norm",
        "vocab_size": 18677,
        "pos_embed_size": 50,
        "pos_embed_path": "../data/WikiQA/embed_glove_pos_d50_norm",
        "pos_vocab_size": 88,
        "train_embed": true,
        "target_mode": "ranking",
        "text1_maxlen": 15,
        "text2_maxlen": 50,
        "pos1_maxlen": 15,
        "pos2_maxlen": 50
    },
    "train": {
        "input_type": "WordPOS_PairGenerator",
        "phase": "TRAIN",
        "use_iter": false,
        "query_per_iter": 50,
        "batch_per_iter": 5,
        "batch_size": 100,
        "relation_file": "../data/WikiQA/relation_train.txt"
    },
    "valid": {
        "input_type": "WordPOS_ListGenerator",
        "phase": "EVAL",
        "batch_list": 10,
        "relation_file": "../data/WikiQA/relation_valid.txt"
    },
    "test": {
        "input_type": "WordPOS_ListGenerator",
        "phase": "EVAL",
        "batch_list": 10,
        "relation_file": "../data/WikiQA/relation_test.txt"
    },
    "predict": {
        "input_type": "WordPOS_ListGenerator",
        "phase": "PREDICT",
        "batch_list": 10,
        "relation_file": "../data/WikiQA/relation_test.txt"
    }
  },
  "outputs": {
    "predict": {
      "save_format": "TREC",
      "save_path": "predict.test.wikiqa.txt"
    }
  },
  "model": {
    "model_path": "../matchzoo/models/",
    "model_py": "geminet.Geminet",
    "setting": {
        "kernel_count": 96,
        "kernel_size": [3, 3],
        "dpool_size": [3, 10],
        "dropout_rate": 0.4,
        "topk": 20,
        "num_layers": 2,
        "hidden_sizes": [5, 1]

    }
  },
  "losses": [ 
    {
       "object_name": "rank_hinge_loss" ,
       "object_params": {
            "margin": 1.0
       }
    }
  ],
  "metrics": [ "ndcg@3", "ndcg@5", "map" ]
}

